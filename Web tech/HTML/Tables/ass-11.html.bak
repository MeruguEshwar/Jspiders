<html>
	<head>
		<title>SIGNUP USING TABLE ALIGNMENT</title>
	</head>
	<body>
			<form>
					<table>
							<tr>
									<td>FIRST NAME</td>
									<td><input type="text"required placeholder="FIRST NAME" title="only character are allowed"></td>
							</tr>

							<tr>
									<td>LAST NAME</td>
									<td><input type="text"required placeholder="LAST NAME" title="only character are allowed"></td>
							</tr>

							<tr>
									<td>EMAIL</td>
									<td><input type="text"required placeholder="EMAIL" title="only character are allowed"></td>
							</tr>

							<tr>
									<td>PASSWARD</td>
									<td><input type="Passward"required placeholder="PASSWARD" ></td>
							</tr>

							<tr>
									<td>CONFORM PASSWARD</td>
									<td><input type="text" placeholder="CONFORM PASS"></td>
							</tr>

							<tr>
									<td>PHONE NUMBER</td>
									<td><input type="text"required placeholder="CELL NUMBER"
									 pattern="[0-9]{10}" title="only character are allowed"></td>
							</tr>
							<tr>
									<td>DATE OF BIRTH</td>
									<td><input type="text"required placeholder="DD/MM/YY" title="only character are allowed"></td>
							</tr>
							<tr>
									<td></td>
									<td></td>
							</tr>

					</table>
			</form>
	</body>
</html>
